using System;

class Sort3NumbersWithNestedIfs
{
    static void Main()
    {
        Console.WriteLine("Enter a,b and c:");
        double a = double.Parse(Console.ReadLine());
        double b = double.Parse(Console.ReadLine());
        double c = double.Parse(Console.ReadLine());
        if (a == b)
        {
            if (b > c)
            {
                Console.WriteLine("{0} {1} {2}", a, a, c);
            }
            else if (b < c)
            {
                Console.WriteLine("{0} {1} {2}", c, a, a);
            }
            else if (b == c)
            {
                Console.WriteLine("{0} {1} {2}", a, a, a);
            }
        }
        else if (b == c)
        {
            if (a > b)
            {
                Console.WriteLine("{0} {1} {2}", a, b, b); ;
            }
            else if (a < b)
            {
                Console.WriteLine("{0} {1} {2}", b, b, a);
            }
        }
        else if (c == a)
        {
            if (b > c)
            {
                Console.WriteLine("{0} {1} {2}", b, c, c);
            }
            else if (b < c)
            {
                Console.WriteLine("{0} {1} {2}", c, c, b);
            }
        }
        else if (a > b)
        {
            if (a > c)
            {
                if (b < c)
                {
                    Console.WriteLine("{0} {1} {2}", a, c, b);
                }
                else if (b>c)
                {
                        Console.WriteLine("{0} {1} {2}", a, b, c);
                }
            }
            else if (a < c)
            {
                if (b < c)
                {
                    Console.WriteLine("{0} {1} {2}", c, a, b);
                }
            }
        }
        else if (a < b)
        {
            if (a < c)
            {
                if (b < c)
                {
                    Console.WriteLine("{0} {1} {2}", c, b, a);
                }
                else if (b > c)
                {
                    Console.WriteLine("{0} {1} {2}", b, c, a);
                }
             }
         }
     }
}


